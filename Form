<script type="text/javascript">
$(document).ready(function(){
    
var selectForm = $('#form_dropdown_SIMPLE_QUESTION_371');
if (selectForm.val() == 121) { $('#esciz').show(); }
    selectForm.on('change', function(){     
        if(selectForm.val() == 121) { $('#esciz').show(); }
        else { $('#esciz').hide(); }
    });
});
    
    function addTrObm () {
        var showValue = +$("#show").attr('nextshow');
        if (showValue == 12) {
          $("#show").hide();
        }
        $('#w'+showValue).show();
        $("#show").attr('nextshow', showValue+1);
        
    }
</script>

